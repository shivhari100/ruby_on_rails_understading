<h1>users details</h1>
<table>
    <tr>
        <th>id</th>
        <th>name</th>
        <th>password</th>
        <th>created_at</th>
        <th>updated_at</th>
    </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.user_name %></td>
      <td><%= user.password %></td>
      <td><%= user.created_at %></td>
      <td><%= user.updated_at %></td>
      <td> <%= link_to 'show', login_show_path(user) %></td>
      <td><%= link_to 'update' ,update_show_path(user) %></td>
      <td><%= link_to 'delete', login_delete_path(user), method: :delete %></td>
      
    </tr>
  <% end %>
  <tr><td><%= link_to 'create', login_new_path %></td></tr>
</table>